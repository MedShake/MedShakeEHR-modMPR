{% set tag = page.courrier %}

<p style="text-align : right;">New York, le {{ tag.date|date("d/m/Y") }}</p>

<h2 style="text-align : center; margin-bottom: 20pt;">Compte-rendu d’une séance de mésothérapie<br>{{ tag.identiteCompleteTitreLongDdn }}</h2>

<h3>Réalisation</h3>

<p>Asepsie : {{ tag.mprCsMesoAsepsie }}</p>
<p>{{ tag.mprCsMesoGeste|nl2br }}</p>

<h3>Information patient</h3>

<p>{{ tag.mprCsMesoInfosPatient|nl2br }}</p>


<br><br><br><br>
<p style="text-align:right">{{ tag.UtilisateurActif_identiteUsuelleTitre }}</p>
